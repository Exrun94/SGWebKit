            <li>
                <div class="collapsible-header"><i class="material-icons">add_circle</i>WordPress</div>
                <div class="collapsible-body">
                    {{#each articles}}
                    {{#is this.category "wordpress"}}
                    {{#is this.panel "cpanel"}}
                    <div class="collection">
                        <a href="{{this.articleUrl}}" class="collection-item" onclick="copyURI(event)"> <i
                                class="material-icons tiny">link</i> {{this.articleName}}
                        </a>
                    </div>
                    {{/is}}
